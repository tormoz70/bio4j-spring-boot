<?xml version="1.0" encoding="UTF-8"?>
<configuration>

    <appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <charset>utf-8</charset>
            <pattern>%date{dd.MM.yyyy HH:mm:ss.SSS}  [%thread]  %-5level  %25.26logger{0}:  %msg %ex{full} %n</pattern>
        </encoder>
    </appender>

    <logger name="ru.open.haven" level="TRACE" />

    <root level="DEBUG">
        <appender-ref ref="CONSOLE" />
    </root>
   </configuration>