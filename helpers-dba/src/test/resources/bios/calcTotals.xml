<?xml version="1.0" encoding="UTF-8"?>
<cursor xmlns="http://bio4j.ru/biodef-repo/store"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://bio4j.ru/biodef-repo/store http://bio4j.ru/biodef-repo/store/store-v11.xsd"
        multiselection="true">
    <SQL action="select">
        <text><![CDATA[
            SELECT 1 AS sumcol, 4 AS avgcol FROM dual
            UNION ALL
            SELECT 8 AS sumcol, 3 AS avgcol FROM dual
            UNION ALL
            SELECT 2 AS sumcol, 1 AS avgcol FROM dual
            UNION ALL
            SELECT 3 AS sumcol, 3 AS avgcol FROM dual
        ]]></text>
    </SQL>
    <fields>
        <field name="sumcol" type="integer"/>
        <field name="avgcol" type="integer" aggregate="AVG"/>
    </fields>
</cursor>